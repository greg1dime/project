{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Education list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Schoolname</th>
                <th>Schooltype</th>
                <th>Graduationyear</th>
                <th>Startyear</th>
                <th>City</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('project_education_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.schoolName }}</td>
                <td>{{ entity.schoolType }}</td>
                <td>{{ entity.graduationYear }}</td>
                <td>{{ entity.startYear }}</td>
                <td>{{ entity.city }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('project_education_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('project_education_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('project_education_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
