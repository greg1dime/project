{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Education</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Schoolname</th>
                <td>{{ entity.schoolName }}</td>
            </tr>
            <tr>
                <th>Schooltype</th>
                <td>{{ entity.schoolType }}</td>
            </tr>
            <tr>
                <th>Graduationyear</th>
                <td>{{ entity.graduationYear }}</td>
            </tr>
            <tr>
                <th>Startyear</th>
                <td>{{ entity.startYear }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ entity.city }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('project_education') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('project_education_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
